@100
"~~~~~~~ ruby startline=3 $%@#$"+
"def foo(x)"+
"  return 3"+
"end"+
"~~~~~~~" {
  text.md {
    fenced.code.md {
      "~~~~~~~": punctuation.md
      " "
      "ruby": language.constant.md
      " "
      "startline=3" {
        other.special.attribute.meta.md {
          "startline": key.keyword.md
          "=": punctuation.md
          "3": value.string.md
        }
      }
      " $%@#$"
      source.embedded.ruby {
        "def foo(x)"
        "  return 3"
        "end"
      }
      "~~~~~~~": punctuation.md
    }
  }
}

