@521
"![foo](/url "title")" {
  text.md {
    link.markup.md {
      "![foo]" {
        image.link.string.md {
          "![": punctuation.md
          "foo"
          "]": punctuation.md
        }
      }
      "(/url "title")" {
        "(": punctuation.md
        destination.link.md {
          "/url": uri.underline.link.md
        }
        " "
        "title" {
          title.link.md {
            """: punctuation.md
            "title"
            """: punctuation.md
          }
        }
        ")": punctuation.md
      }
    }
  }
}

